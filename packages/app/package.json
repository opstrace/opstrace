  "scripts": {
    "client:start": "node scripts/startClient.js",
    "client:build": "node scripts/buildClient.js",
    "client:build-debug-workbox": "node scripts/buildClient.js --workbox-debug",
    "client:test": "node scripts/test.js",
    "server:start": "NODE_ENV=development webpack --watch --config config/webpack.server.config.js",
    "server:build": "NODE_ENV=production webpack --config config/webpack.server.config.js",
    "clean": "rimraf build dist .cache tsconfig.tsbuildinfo",
    "build": "yarn client:build && yarn server:build",
    "test": "yarn client:test",
    "lint": "eslint --ext .ts,.tsx src",
    "prebuild": "yarn clean",
    "storybook": "STORYBOOK_GRAPHQL_ENDPOINT=http://localhost:8080/v1/graphql STORYBOOK_GRAPHQL_SUBSCRIPTION_ENDPOINT=ws://localhost:8080/v1/graphql start-storybook -p 9009 -s public --quiet",
    "metadata:export": "yarn hasura metadata export --admin-secret myadminsecret",
    "types:watch": "HASURA_GRAPHQL_ADMIN_SECRET=myadminsecret graphql-codegen --config codegen.js --watch",
    "console": "hasura console --admin-secret myadminsecret",
    "services:start": "docker-compose up"
  },
  "proxy": {
    "/_": {
      "target": "http://localhost:3001",
      "ws": true
    },
    "/modules": {
      "target": "http://localhost:3001"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/config/jest/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/../../node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!monaco-editor).+\\\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^client/(.*)$": "<rootDir>/src/client/$1",
      "^server/(.*)$": "<rootDir>/src/server/$1",
      "^state/(.*)$": "<rootDir>/src/state/$1",
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy",
      "monaco-editor": "<rootDir>/../../node_modules/monaco-editor/esm/vs/editor/editor.api",
      "modulescript/ms.worker": "<rootDir>/src/client/components/Editor/lib/workers/modulescript/ms.worker.ts"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "mjs",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  }
}
