  "scripts": {
    "client:start": "node scripts/startClient.js",
    "client:remote": "REMOTE_DEV=true node scripts/startClient.js",
    "client:build": "node scripts/buildClient.js",
    "client:build-debug-workbox": "node scripts/buildClient.js --workbox-debug",
    "client:test": "node scripts/test.js --modulePathIgnorePatterns src/server",
    "server:start": "NODE_ENV=development webpack --watch --config config/webpack.server.config.js",
    "server:remote": "OPSTRACE_BUILDINFO_PATH=package.json NODE_ENV=production REMOTE_DEV=true webpack --watch --config config/webpack.server.config.js",
    "server:build": "NODE_ENV=production webpack --config config/webpack.server.config.js",
    "server:test": "node scripts/test.js --env=node --roots=src/server",
    "clean": "rimraf build dist .cache tsconfig.tsbuildinfo",
    "build": "yarn client:build && yarn server:build",
    "test": "yarn client:test && yarn server:test",
    "lint": "eslint --ext .ts,.tsx src",
    "typecheck": "tsc --noEmit",
    "prebuild": "yarn clean",
    "metadata:export": "yarn hasura metadata export --admin-secret myadminsecret",
    "types": "HASURA_GRAPHQL_ADMIN_SECRET=myadminsecret graphql-codegen --config codegen.js",
    "types:watch": "yarn types --watch",
    "console": "hasura console --admin-secret myadminsecret --no-browser",
    "services:start": "docker-compose up",
    "lib:gen": "ts-node src/sdk/compiler.ts",
    "lib:watch": "ts-node src/sdk/compiler.ts --watch",
    "migrate": "yarn hasura migrate apply --admin-secret myadminsecret",
    "migrate:down": "yarn hasura migrate apply --down 1 --admin-secret myadminsecret",
    "migrate:up": "yarn hasura migrate apply --up 1 --admin-secret myadminsecret",
    "seeds:dev:tenants": "yarn hasura seeds apply --file dev/default_tenants.sql --admin-secret myadminsecret",
    "remoteEnv": "GRAPHQL_ENDPOINT=http://127.0.0.1:8090/v1/graphql GRAPHQL_ENDPOINT_PORT=8090 HASURA_GRAPHQL_ENDPOINT=http://127.0.0.1:8090 HASURA_GRAPHQL_ADMIN_SECRET=$(kubectl get secrets -n kube-system -o json hasura-admin-secret | jq -r .data.HASURA_ADMIN_SECRET | base64 -d) REDIS_PASSWORD=$(kubectl get secrets -n application -o json redis-password | jq -r .data.REDIS_MASTER_PASSWORD | base64 -d)",
    "remote:seeds:dev:tenants": "yarn remoteEnv yarn seeds:dev:tenants",
    "remote:services:start:graphql": "kubectl port-forward -n application deployment/graphql 8090:8080",
    "remote:services:start:redis": "kubectl port-forward -n application deployment/redis-master 6379",
    "remote:server:start": "OPSTRACE_BUILDINFO_PATH=package.json yarn remoteEnv yarn server:start",
    "remote:console": "yarn remoteEnv yarn hasura console --no-browser",
    "remote:migrate": "yarn remoteEnv yarn hasura migrate apply",
    "remote:migrate:down": "yarn remoteEnv yarn hasura migrate apply --down 1",
    "remote:migrate:up": "yarn remoteEnv yarn hasura migrate apply --up 1",
    "telepresenceEnv": "GRAPHQL_ENDPOINT=http://graphql.application.svc.cluster.local:8080/v1/graphql GRAPHQL_ENDPOINT_PORT=8080 HASURA_GRAPHQL_ENDPOINT=http://graphql.application.svc.cluster.local:8080 HASURA_GRAPHQL_ADMIN_SECRET=$(kubectl get secrets -n kube-system -o json hasura-admin-secret | jq -r .data.HASURA_ADMIN_SECRET | base64 -d)",
    "telepresence:console": "yarn telepresenceEnv yarn hasura console --no-browser",
    "telepresence:migrate": "yarn telepresenceEnv yarn hasura migrate apply",
    "telepresence:migrate:down": "yarn telepresenceEnv yarn hasura migrate apply --down 1",
    "telepresence:migrate:up": "yarn telepresenceEnv yarn hasura migrate apply --up 1",
    "telepresence:seeds:dev:tenants": "yarn telepresenceEnv yarn seeds:dev:tenants",
    "telepresence:types": "yarn telepresenceEnv yarn graphql-codegen --config codegen.js"
  },
  "eslintIgnore": [],
  "proxy": {
    "/_": {
      "target": "http://localhost:3001",
      "ws": true
    },
    "/modules": {
      "target": "http://localhost:3001"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/config/jest/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/../../node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!monaco-editor).+\\\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^client/(.*)$": "<rootDir>/src/client/$1",
      "^server/(.*)$": "<rootDir>/src/server/$1",
      "^state/(.*)$": "<rootDir>/src/state/$1",
      "^workers$": "<rootDir>/src/workers/index",
      "^workers/(.*)$": "<rootDir>/src/workers/$1",
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy",
      "monaco-editor": "<rootDir>/../../node_modules/monaco-editor/esm/vs/editor/editor.api"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "mjs",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  }
}
