{
  "name": "@opstrace/e2e-testing",
  "version": "0.0.0",
  "description": "Opstrace e2e testing",
  "scripts": {
    "cypress:open": "cypress open",
    "lint": "eslint . --ext .js",
    "clean": "rimraf build .cache tsconfig.tsbuildinfo"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "rules": {
      "cypress/no-unnecessary-waiting": "off"
    },
    "extends": [
      "plugin:cypress/recommended"
    ]
  },
  "devDependencies": {
    "cypress": "^5.5.0",
    "eslint-plugin-cypress": "^2.11.2"
  }
}
